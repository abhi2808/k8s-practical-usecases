{
  "__inputs": [],
  "__requires": [],
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [
    {
      "icon": "arrow-left",
      "tags": [],
      "title": "← Back to Monitor-1",
      "tooltip": "Return to client list",
      "type": "link",
      "url": "/d/monitor-1/monitor-1"
    }
  ],
  "panels": [
    {
      "type": "stat",
      "title": "Nodes",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
      "options": { "colorMode": "background", "graphMode": "none", "justifyMode": "center", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": { "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] }, "mappings": [] },
        "overrides": []
      },
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "expr": "count(kube_node_info{clientName=\"$client\"})",
        "instant": true, "refId": "A"
      }],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "stat",
      "title": "Running Pods",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 },
      "options": { "colorMode": "background", "graphMode": "none", "justifyMode": "center", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": { "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "mappings": [] },
        "overrides": []
      },
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "expr": "count(kube_pod_status_phase{clientName=\"$client\", phase=\"Running\"} == 1)",
        "instant": true, "refId": "A"
      }],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "stat",
      "title": "Pending Pods",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 },
      "options": { "colorMode": "background", "graphMode": "none", "justifyMode": "center", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": { "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 1 }] }, "mappings": [] },
        "overrides": []
      },
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "expr": "count(kube_pod_status_phase{clientName=\"$client\", phase=\"Pending\"} == 1) or vector(0)",
        "instant": true, "refId": "A"
      }],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "stat",
      "title": "Failed Pods",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 },
      "options": { "colorMode": "background", "graphMode": "none", "justifyMode": "center", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": { "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 1 }] }, "mappings": [] },
        "overrides": []
      },
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "expr": "count(kube_pod_status_phase{clientName=\"$client\", phase=\"Failed\"} == 1) or vector(0)",
        "instant": true, "refId": "A"
      }],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "stat",
      "title": "CPU Usage %",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
      "options": { "colorMode": "background", "graphMode": "area", "justifyMode": "center", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": { "unit": "percent", "decimals": 1, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 60 }, { "color": "red", "value": 85 }] }, "mappings": [] },
        "overrides": []
      },
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "expr": "sum(rate(node_cpu_seconds_total{clientName=\"$client\", mode!=\"idle\"}[5m])) / sum(rate(node_cpu_seconds_total{clientName=\"$client\"}[5m])) * 100",
        "instant": true, "refId": "A"
      }],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "stat",
      "title": "Memory Usage %",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 },
      "options": { "colorMode": "background", "graphMode": "area", "justifyMode": "center", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": { "unit": "percent", "decimals": 1, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 70 }, { "color": "red", "value": 90 }] }, "mappings": [] },
        "overrides": []
      },
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "expr": "sum(node_memory_MemTotal_bytes{clientName=\"$client\"} - node_memory_MemAvailable_bytes{clientName=\"$client\"}) / sum(node_memory_MemTotal_bytes{clientName=\"$client\"}) * 100",
        "instant": true, "refId": "A"
      }],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "timeseries",
      "title": "CPU Usage per Node",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "options": { "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "fieldConfig": {
        "defaults": { "unit": "percent", "min": 0, "custom": { "lineWidth": 2, "fillOpacity": 10 } },
        "overrides": []
      },
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "expr": "sum by (instance) (rate(node_cpu_seconds_total{clientName=\"$client\", mode!=\"idle\"}[5m])) / sum by (instance) (rate(node_cpu_seconds_total{clientName=\"$client\"}[5m])) * 100",
        "legendFormat": "{{instance}}",
        "refId": "A"
      }],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "timeseries",
      "title": "Memory Usage per Node",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "options": { "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "fieldConfig": {
        "defaults": { "unit": "bytes", "min": 0, "custom": { "lineWidth": 2, "fillOpacity": 10 } },
        "overrides": []
      },
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "expr": "node_memory_MemTotal_bytes{clientName=\"$client\"} - node_memory_MemAvailable_bytes{clientName=\"$client\"}",
        "legendFormat": "{{instance}}",
        "refId": "A"
      }],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "table",
      "title": "Node Stats",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 12 },
      "options": { "cellHeight": "md", "footer": { "show": false }, "showHeader": true },
      "fieldConfig": {
        "defaults": { "custom": { "align": "left", "cellOptions": { "type": "auto" } } },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "CPU %" },
            "properties": [
              { "id": "unit", "value": "percent" },
              { "id": "custom.cellOptions", "value": { "type": "gauge", "mode": "gradient" } },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 60 }, { "color": "red", "value": 85 }] } },
              { "id": "min", "value": 0 }, { "id": "max", "value": 100 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Memory %" },
            "properties": [
              { "id": "unit", "value": "percent" },
              { "id": "custom.cellOptions", "value": { "type": "gauge", "mode": "gradient" } },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 70 }, { "color": "red", "value": 90 }] } },
              { "id": "min", "value": 0 }, { "id": "max", "value": 100 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Disk Available" },
            "properties": [{ "id": "unit", "value": "bytes" }]
          }
        ]
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum by (instance) (rate(node_cpu_seconds_total{clientName=\"$client\", mode!=\"idle\"}[5m])) / sum by (instance) (rate(node_cpu_seconds_total{clientName=\"$client\"}[5m])) * 100",
          "format": "table", "instant": true, "legendFormat": "__auto", "refId": "CPU"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "(node_memory_MemTotal_bytes{clientName=\"$client\"} - node_memory_MemAvailable_bytes{clientName=\"$client\"}) / node_memory_MemTotal_bytes{clientName=\"$client\"} * 100",
          "format": "table", "instant": true, "legendFormat": "__auto", "refId": "Mem"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "node_filesystem_avail_bytes{clientName=\"$client\", mountpoint=\"/\"}",
          "format": "table", "instant": true, "legendFormat": "__auto", "refId": "Disk"
        }
      ],
      "transformations": [
        { "id": "joinByField", "options": { "byField": "instance", "mode": "outer" } },
        { "id": "filterFieldsByName", "options": { "include": { "names": ["instance", "Value #CPU", "Value #Mem", "Value #Disk"] } } },
        { "id": "organize", "options": { "renameByName": { "instance": "Node", "Value #CPU": "CPU %", "Value #Mem": "Memory %", "Value #Disk": "Disk Available" } } }
      ],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "table",
      "title": "Running Pods by Namespace",
      "description": "All running pods. Click the filter icon on the Namespace column to filter by namespace.",
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 20 },
      "options": { "cellHeight": "sm", "footer": { "show": false }, "showHeader": true },
      "fieldConfig": {
        "defaults": { "custom": { "align": "left", "filterable": true } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Namespace" }, "properties": [{ "id": "custom.width", "value": 150 }] },
          { "matcher": { "id": "byName", "options": "Pod" }, "properties": [{ "id": "custom.width", "value": 280 }] },
          {
            "matcher": { "id": "byName", "options": "Status" },
            "properties": [
              { "id": "custom.width", "value": 100 },
              { "id": "custom.cellOptions", "value": { "type": "color-background" } },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] } }
            ]
          }
        ]
      },
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "expr": "kube_pod_status_phase{clientName=\"$client\", phase=\"Running\"} == 1",
        "format": "table", "instant": true, "legendFormat": "__auto", "refId": "A"
      }],
      "transformations": [
        { "id": "filterFieldsByName", "options": { "include": { "names": ["namespace", "pod", "phase"] } } },
        { "id": "organize", "options": { "renameByName": { "namespace": "Namespace", "pod": "Pod", "phase": "Status" }, "indexByName": { "namespace": 0, "pod": 1, "phase": 2 } } },
        { "id": "sortBy", "options": { "fields": [{ "desc": false, "displayName": "Namespace" }] } }
      ],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "table",
      "title": "Failed / Unhealthy Pods + Reason",
      "description": "Pods not running or succeeded, with the reason they are down.",
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 20 },
      "options": { "cellHeight": "sm", "footer": { "show": false }, "showHeader": true },
      "fieldConfig": {
        "defaults": { "custom": { "align": "left", "filterable": true } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Namespace" }, "properties": [{ "id": "custom.width", "value": 130 }] },
          { "matcher": { "id": "byName", "options": "Pod" }, "properties": [{ "id": "custom.width", "value": 220 }] },
          {
            "matcher": { "id": "byName", "options": "Phase" },
            "properties": [
              { "id": "custom.width", "value": 100 },
              { "id": "custom.cellOptions", "value": { "type": "color-background" } },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "red", "value": null }] } }
            ]
          },
          { "matcher": { "id": "byName", "options": "Reason" }, "properties": [{ "id": "custom.width", "value": 160 }] }
        ]
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "kube_pod_status_phase{clientName=\"$client\", phase!=\"Running\", phase!=\"Succeeded\"} == 1",
          "format": "table", "instant": true, "legendFormat": "__auto", "refId": "Phase"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "kube_pod_status_reason{clientName=\"$client\"} == 1",
          "format": "table", "instant": true, "legendFormat": "__auto", "refId": "Reason"
        }
      ],
      "transformations": [
        { "id": "joinByField", "options": { "byField": "pod", "mode": "outer" } },
        { "id": "filterFieldsByName", "options": { "include": { "names": ["namespace", "pod", "phase", "reason"] } } },
        { "id": "filterByValue", "options": { "filters": [{ "config": { "id": "isNotNull", "options": {} }, "fieldName": "phase" }], "match": "all", "type": "include" } },
        { "id": "organize", "options": { "renameByName": { "namespace": "Namespace", "pod": "Pod", "phase": "Phase", "reason": "Reason" }, "indexByName": { "namespace": 0, "pod": 1, "phase": 2, "reason": 3 } } }
      ],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "table",
      "title": "Container Restarts & Waiting Reason",
      "description": "Containers with restarts or stuck waiting — shows exact reason (CrashLoopBackOff, ImagePullBackOff, OOMKilled etc.)",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 30 },
      "options": { "cellHeight": "sm", "footer": { "show": false }, "showHeader": true },
      "fieldConfig": {
        "defaults": { "custom": { "align": "left", "filterable": true } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Namespace" }, "properties": [{ "id": "custom.width", "value": 130 }] },
          { "matcher": { "id": "byName", "options": "Pod" }, "properties": [{ "id": "custom.width", "value": 220 }] },
          { "matcher": { "id": "byName", "options": "Container" }, "properties": [{ "id": "custom.width", "value": 160 }] },
          {
            "matcher": { "id": "byName", "options": "Restarts" },
            "properties": [
              { "id": "custom.width", "value": 90 },
              { "id": "custom.cellOptions", "value": { "type": "color-text" } },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 1 }, { "color": "red", "value": 5 }] } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Waiting Reason" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "type": "color-background" } },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "orange", "value": null }] } }
            ]
          }
        ]
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "kube_pod_container_status_restarts_total{clientName=\"$client\"} > 0",
          "format": "table", "instant": true, "legendFormat": "__auto", "refId": "Restarts"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "kube_pod_container_status_waiting_reason{clientName=\"$client\"} == 1",
          "format": "table", "instant": true, "legendFormat": "__auto", "refId": "WaitReason"
        }
      ],
      "transformations": [
        { "id": "joinByField", "options": { "byField": "pod", "mode": "outer" } },
        { "id": "filterFieldsByName", "options": { "include": { "names": ["namespace", "pod", "container", "Value #Restarts", "reason"] } } },
        { "id": "organize", "options": { "renameByName": { "namespace": "Namespace", "pod": "Pod", "container": "Container", "Value #Restarts": "Restarts", "reason": "Waiting Reason" }, "indexByName": { "namespace": 0, "pod": 1, "container": 2, "Value #Restarts": 3, "reason": 4 } } },
        { "id": "sortBy", "options": { "fields": [{ "desc": true, "displayName": "Restarts" }] } }
      ],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "bargauge",
      "title": "Pods per Namespace",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 38 },
      "options": { "displayMode": "gradient", "minVizHeight": 10, "minVizWidth": 0, "orientation": "horizontal", "reduceOptions": { "calcs": ["lastNotNull"] }, "valueMode": "color" },
      "fieldConfig": { "defaults": { "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] }, "mappings": [] }, "overrides": [] },
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "expr": "count by (namespace) (kube_pod_info{clientName=\"$client\"})",
        "instant": true, "legendFormat": "{{namespace}}", "refId": "A"
      }],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "bargauge",
      "title": "CPU Requests vs Limits by Namespace",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 38 },
      "options": { "displayMode": "gradient", "orientation": "horizontal", "reduceOptions": { "calcs": ["lastNotNull"] }, "valueMode": "color" },
      "fieldConfig": { "defaults": { "unit": "cores", "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "mappings": [] }, "overrides": [] },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum by (namespace) (kube_pod_container_resource_requests{clientName=\"$client\", resource=\"cpu\"})",
          "instant": true, "legendFormat": "Requests: {{namespace}}", "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum by (namespace) (kube_pod_container_resource_limits{clientName=\"$client\", resource=\"cpu\"})",
          "instant": true, "legendFormat": "Limits: {{namespace}}", "refId": "B"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "timeseries",
      "title": "Network Receive (bytes/s) per Node",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 46 },
      "options": { "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "fieldConfig": { "defaults": { "unit": "Bps", "min": 0, "custom": { "lineWidth": 2, "fillOpacity": 10 } }, "overrides": [] },
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "expr": "rate(node_network_receive_bytes_total{clientName=\"$client\", device!~\"lo|veth.*|docker.*|flannel.*|cali.*|cbr.*\"}[5m])",
        "legendFormat": "{{instance}} - {{device}}", "refId": "A"
      }],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "timeseries",
      "title": "Network Transmit (bytes/s) per Node",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 46 },
      "options": { "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "fieldConfig": { "defaults": { "unit": "Bps", "min": 0, "custom": { "lineWidth": 2, "fillOpacity": 10 } }, "overrides": [] },
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "expr": "rate(node_network_transmit_bytes_total{clientName=\"$client\", device!~\"lo|veth.*|docker.*|flannel.*|cali.*|cbr.*\"}[5m])",
        "legendFormat": "{{instance}} - {{device}}", "refId": "A"
      }],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "gauge",
      "title": "CPU Requests Utilization",
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 54 },
      "options": { "minVizHeight": 75, "minVizWidth": 75, "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"] }, "showThresholdLabels": false, "showThresholdMarkers": true },
      "fieldConfig": { "defaults": { "unit": "percent", "min": 0, "max": 150, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 80 }, { "color": "red", "value": 100 }] }, "mappings": [] }, "overrides": [] },
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "expr": "sum(rate(node_cpu_seconds_total{clientName=\"$client\", mode!=\"idle\"}[5m])) / sum(kube_pod_container_resource_requests{clientName=\"$client\", resource=\"cpu\"}) * 100",
        "instant": true, "refId": "A"
      }],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "type": "gauge",
      "title": "Memory Requests Utilization",
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 54 },
      "options": { "minVizHeight": 75, "minVizWidth": 75, "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"] }, "showThresholdLabels": false, "showThresholdMarkers": true },
      "fieldConfig": { "defaults": { "unit": "percent", "min": 0, "max": 150, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 80 }, { "color": "red", "value": 100 }] }, "mappings": [] }, "overrides": [] },
      "targets": [{
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "expr": "sum(node_memory_MemTotal_bytes{clientName=\"$client\"} - node_memory_MemAvailable_bytes{clientName=\"$client\"}) / sum(kube_pod_container_resource_requests{clientName=\"$client\", resource=\"memory\"}) * 100",
        "instant": true, "refId": "A"
      }],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["monitoring", "detail"],
  "templating": {
    "list": [
      {
        "current": {},
        "hide": 0,
        "includeAll": false,
        "name": "datasource",
        "options": [],
